   <string notr="true">/* Styl dla całego okna głównego (QWidget lub QMainWindow) */
QWidget {
    background-color: #2e2e2e; /* Ciemnoszary tło */
    color: #ffffff;           /* Biały tekst */
    font-family: Arial, sans-serif;
    font-size: 10pt;
}

/* Style dla list (QListWidget) */
QListWidget {
    background-color: #3a3a3a; /* Trochę jaśniejsze tło dla lepszego kontrastu */
    border: 1px solid #5c5c5c;
    selection-background-color: #0078d7; /* Kolor zaznaczenia (niebieski) */
    selection-color: #ffffff;
    padding: 5px;
}

QListWidget::item:selected {
    background-color: #0078d7;
    color: #ffffff;
}

/* Style dla etykiet (QLabel) */
QLabel {
    color: #ffffff;
    padding: 2px 0;
}

/* Style dla przycisków (QPushButton) */
QPushButton {
    background-color: #0078d7; /* Niebieskie tło */
    color: #ffffff;           /* Biały tekst */
    border: none;
    padding: 8px 16px;
    border-radius: 4px;
    font-weight: bold;
}

QPushButton:hover {
    background-color: #005a9e; /* Ciemniejszy niebieski po najechaniu */
}

QPushButton:pressed {
    background-color: #003c6e; /* Jeszcze ciemniejszy po kliknięciu */
}

/* Style dla GroupBox */
QGroupBox {
    border: 1px solid #5c5c5c;
    margin-top: 10px; /* Zostaw miejsce na tytuł */
    padding: 10px;
    background-color: #3a3a3a;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 0 5px;
    color: #ffffff;
    background-color: #3a3a3a;
}

/* Style dla pól wyboru (QCheckBox) */
QCheckBox {
    color: #ffffff;
}

QCheckBox::indicator {
    width: 13px;
    height: 13px;
}

QCheckBox::indicator:unchecked {
    background-color: #3a3a3a;
    border: 1px solid #5c5c5c;
}

QCheckBox::indicator:checked {
    background-color: #0078d7; /* Niebieski po zaznaczeniu */
    border: 1px solid #0078d7;
    image: url(path/to/check.png); /* Własna ikona zaznaczenia (opcjonalnie) */
}

/* Style dla edytora tekstu (QTextEdit) */
QTextEdit {
    background-color: #3a3a3a;
    color: #ffffff;
    border: 1px solid #5c5c5c;
    padding: 5px;
}</string>
